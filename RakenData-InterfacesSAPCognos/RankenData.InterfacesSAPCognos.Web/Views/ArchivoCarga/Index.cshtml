@model IEnumerable<RankenData.InterfacesSAPCognos.Web.Models.ArchivoCarga>

@{
    ViewBag.Title = "Monitor de Carga";
}

<h2>Monitor de Carga</h2>

<p>
    @Html.ActionLink("Crear Nuevo", "Create")
</p>
@using (Html.BeginForm())
{
    <p>     
        Fecha Inicio @Html.TextBox("startDate")
        Fecha Inicio @Html.TextBox("endDate")
        <input type="submit" value="Buscar" />
    </p>
}
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Identificador)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Anio_Col3)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Mes_Col4)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TipoArchivoCarga1.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.Username)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Identificador)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Anio_Col3)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Mes_Col4)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TipoArchivoCarga1.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.Username)
        </td>
        <td>           
            @Html.ActionLink("Detalles", "Details", new { id=item.Id }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>

@model RankenData.InterfacesSAPCognos.Web.Models.ArchivoCarga

@{
    ViewBag.Title = "Detalle";
}

<h2>Detalle</h2>

<div>
    <h4>Archivo Carga</h4>
	<hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Nombre)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Identificador)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Identificador)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Fecha)
            
        </dt>

        <dd>            
            @Model.Fecha.ToString("dd/MM/yyyy")
        </dd>

        <dt>            
            @Html.Label("Año")            
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Anio_Col3)
        </dd>

        <dt>            
            @Html.Label("Período")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Mes_Col4)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.TipoArchivoCarga1.Nombre)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TipoArchivoCarga1.Nombre)
        </dd>

        <dt>
            @Html.Label("Usuario")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.User.Username)
        </dd>

    </dl>
</div>
<p>    
    @Html.ActionLink("Regresar a la lista", "Index")
</p>

@model RankenData.InterfacesSAPCognos.Web.Models.CargaAutomatica

@{
    ViewBag.Title = "Borrar";
}

<h2>Borrar</h2>

<h3>Esta seguro de eliminar esto?</h3>
<div>
    <h4>Programar Carga</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @*@Html.DisplayNameFor(model => model.FechaProgramada)*@
            @Html.DisplayName("Fecha programada")
        </dt>

        <dd>
            @*@Html.DisplayFor(model => model.FechaProgramada)*@
            @Model.FechaProgramada.ToString("dd/MM/yyyy")
        </dd>

        <dt>
            @*@Html.DisplayNameFor(model => model.RutaArchivo)*@
            @Html.DisplayName("Ruta archivo")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.RutaArchivo)
        </dd>

        <dt>
            @*@Html.DisplayNameFor(model => model.Usuario)*@
            @Html.DisplayName("Usuario")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Usuario)
        </dd>

        <dt>
            @*@Html.DisplayNameFor(model => model.TipoArchivoCarga.Nombre)*@
            @Html.DisplayName("Email")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Borrar" class="btn btn-default" /> |
            @Html.ActionLink("Regresar a la lista", "Index")
        </div>
    }
</div>

@model RankenData.InterfacesSAPCognos.Web.Models.SaldoInicial

@{
    ViewBag.Title = "Borrar";
}

<h2>Borrar</h2>

<h3>Esta seguro de eliminar esto?</h3>
<div>
    <h4>Saldo Iicial</h4>
    <hr />
    <dl class="dl-horizontal">

        <dt>           
            @Html.Label("Cuenta Cognos")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CuentaCognosValue)
        </dd>

        <dt>           
            @Html.Label("Compania Cognos")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CompaniaCognosValue)
        </dd>

        <dt>
           @Html.Label("Anio Fiscal")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.AnioFiscalValue)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Saldo)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Saldo)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Borrar" class="btn btn-default" /> |
            @Html.ActionLink("Regresar a la lista", "Index")
        </div>
    }
</div>
